package park;

import java.io.FileNotFoundException;

import elements.Car;
import elements.LinkedList;
import elements.Stack;
import io.In;

public class park {
	Stack<Car> place = new Stack<Car>();
	LinkedList<Car> road = new LinkedList<Car>();
	private int s_max = 3;
	private int s_count = 0;
	private int count = 0;

	void change_smax(int a) {
		this.s_max=a;
	}
	int size() {
		return count;
	}
	void in() {
		In in = new In();
		try {
			in.input();
		} catch (FileNotFoundException e) {
			// TODO 自动生成的 catch 块
			e.printStackTrace();
		}
		if (in.back().size() > 0) {
			for (int i = 0; i < in.back().size(); i++) {
				if (i < s_max) {
					place.push(in.back().get(i));
					s_count++;
				} else {
					road.add(in.back().get(i));
					count++;
				}
			}
			count += s_count; 
		}
	}
	void in(String str) {
		Car tmp=new Car(str);
		if(s_count<s_max)
			place.push(tmp);
		else
			road.add(tmp);
		}

	void out() {

	}
}
