package park;

import elements.Car;

public class manager {
	private String pwd="";
	private double price=1.0;
	park p= new park();
	public manager(){
		this.pwd="123456";
		p.file_in();
	}
	public void change_pwd(String NEW) {
		pwd=NEW;
	}
	public void chenge_max(int a) {
		p.change_smax(a);
	}
	public void parking(String str) {
		p.in(str);
	}
	public String leaving(String str) {
		return p.out(str);
	}
	public double price() {
		return price;
	}
	public double fee(Car o) {
		double fee=0.0;
		if (o.info) {
			long endtime = System.currentTimeMillis();
			long usedtime = (endtime - o.time) / (1000 * 60);
			if (usedtime  > 0) {
				fee = usedtime * price;
			} 
		}
		return fee;
	}
	public String login() {
		return pwd;
	}

}
